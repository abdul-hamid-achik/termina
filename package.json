{
  "name": "termina",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A text-based multiplayer MOBA played through a terminal-inspired browser UI",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration",
    "test:e2e": "playwright test --config playwright.config.ts",
    "test:e2e:ui": "playwright test --config playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config playwright.config.ts --headed",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "knip": "knip",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "nuxt": "^3.14.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0",
    "effect": "^3.12.0",
    "@effect/schema": "^0.75.0",
    "@effect/platform": "^0.72.0",
    "pinia": "^2.3.0",
    "@pinia/nuxt": "^0.9.0",
    "drizzle-orm": "^0.38.0",
    "postgres": "^3.4.0",
    "ioredis": "^5.4.0",
    "nuxt-auth-utils": "^0.5.0",
    "@fontsource/jetbrains-mono": "^5.1.0",
    "reka-ui": "^2.1.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.0",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@playwright/test": "^1.58.2",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9.17.0",
    "happy-dom": "^15.0.0",
    "knip": "^5.85.0",
    "lefthook": "^2.1.1",
    "prettier": "^3.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}

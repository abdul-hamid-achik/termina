{
  "name": "termina",
  "version": "0.1.0",
  "private": true,
  "description": "A text-based multiplayer MOBA played through a terminal-inspired browser UI",
  "scripts": {
    "dev": "bun --bun nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration",
    "test:e2e": "vitest run --project e2e",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "nuxt": "^3.14.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0",
    "effect": "^3.12.0",
    "@effect/schema": "^0.75.0",
    "@effect/platform": "^0.72.0",
    "pinia": "^2.3.0",
    "@pinia/nuxt": "^0.9.0",
    "drizzle-orm": "^0.38.0",
    "postgres": "^3.4.0",
    "ioredis": "^5.4.0",
    "nuxt-auth-utils": "^0.5.0",
    "@fontsource/jetbrains-mono": "^5.1.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.0",
    "vitest": "^2.1.0",
    "@vue/test-utils": "^2.4.0",
    "happy-dom": "^15.0.0",
    "drizzle-kit": "^0.30.0",
    "typescript": "^5.7.0",
    "@unocss/nuxt": "^0.65.0",
    "unocss": "^0.65.0"
  }
}
